    "graphql-tag": "^2.12.6",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mongoose": "^8.1.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-toolkit": "^2.1.3",

    "@types/luxon": "^3.3.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.15.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.25",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "bootstrap": "^5.2.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",

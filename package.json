{
  "name": "event-scheduler",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "run-p --race start:server start:web",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "start:server": "cd api && ts-node-dev --respawn --transpile-only ./index.ts && wait-on tcp:4000",
    "start:web": "react-scripts start",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' 'api/**/*.{js,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}' 'api/**/*.{js,ts}'",
    "format": "prettier --write \"./**/*.{js,jsx,css,tsx,ts,json,md,yml}\" --config ./.prettierrc",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "prepare": "husky install"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "dependencies": {

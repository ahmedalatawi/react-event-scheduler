{
  "name": "event-scheduler",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "run-p --race start:server start:web",
    "build": "tsc && vite build",
    "test": "jest",
    "test:coverage": "jest --watchAll=false --coverage",
    "start:server": "cd api && ts-node-dev --respawn --transpile-only ./index.ts && wait-on tcp:4000",
    "start:web": "vite --host --open",
    "ts-check": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,css,tsx,ts,json,md,yml}\" --config ./.prettierrc",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "prepare": "husky install"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@atawi/react-datatable": "^1.0.0",
    "@atawi/react-popover": "^1.0.4",
    "@fullcalendar/core": "^6.1.5",
    "@fullcalendar/daygrid": "^6.1.4",
    "@fullcalendar/interaction": "^6.1.4",
    "@fullcalendar/react": "^6.1.4",
    "@fullcalendar/timegrid": "^6.1.4",
    "@types/styled-components": "^5.1.26",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mongoose": "^8.1.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.0",
    "react-darkreader": "^1.5.6",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^5.0.1",
    "react-idle-timer": "^5.5.2",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.10.0",
    "react-share": "^5.1.0",
    "styled-components": "^6.1.9",
    "ts-node": "^10.9.2"
